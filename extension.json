{
	"name": "GoogleRichCards",
	"version": "0.2",
	"author": [
		"Igor Shishkin",
		"..."
	],
	"url": "https://mediawiki.org/wiki/Extension:GoogleRichCards",
	"description": "Google Rich Cards extension",
	"license-name": "GPL-2.0-only",
	"type": "parserhook",
	"MessagesDirs": {},
	"requires": {
		"MediaWiki": ">= 1.43.0"
	},
	"config": {
		"GoogleRichCardsAnnotateArticles": {
			"description": "Enable annotations for articles (content pages)?",
			"value": true
		},
		"GoogleRichCardsAnnotateEvents": {
			"description": "Enable annotations for events?",
			"value": false
		},
		"GoogleRichCardsAnnotateWebSite": {
			"description": "Enable WebSite annotations?",
			"value": true
		}
	},
	"AutoloadClasses": {
		"MediaWiki\\Extension\\GoogleRichCards\\Hooks": "Hooks.php",
		"MediaWiki\\Extension\\GoogleRichCards\\Article": "Article.php",
		"MediaWiki\\Extension\\GoogleRichCards\\Event": "Event.php",
		"MediaWiki\\Extension\\GoogleRichCards\\WebSite": "WebSite.php"
	},
	"Hooks": {
		"BeforePageDisplay": "MediaWiki\\Extension\\GoogleRichCards\\Hooks::onBeforePageDisplay",
		"ParserFirstCallInit": "MediaWiki\\Extension\\GoogleRichCards\\Hooks::onParserFirstCallInit"
	},
	"manifest_version": 2
}
